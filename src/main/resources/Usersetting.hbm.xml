<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="base.model.Usersetting" table="usersetting" schema="wuziqi">
		<id name="id">
			<column name="_id" sql-type="int(255)"/>
		</id>
		<property name="userid">
			<column name="userid" sql-type="int(255)"/>
		</property>
		<property name="pagesize">
			<column name="pagesize" sql-type="int(11)" not-null="true"/>
		</property>
		<property name="showOnlinecounts">
			<column name="show_onlinecounts" sql-type="bit(1)" not-null="true"/>
		</property>
		<property name="friendtips">
			<column name="friendtips" sql-type="bit(1)" not-null="true"/>
		</property>
		<property name="showmyinfo">
			<column name="showmyinfo" sql-type="bit(1)" not-null="true"/>
		</property>
	</class>
</hibernate-mapping>